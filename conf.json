{
  "message": "Default settings",
  "workingDir": ".",
  "db": {
    "location": {
      "neo4jConf": "config/neo4j.conf",
      "neo4jDB": "neo4jDB/",
      "logFile": "output/logs/run.log",
      "exportDir": "output/results/"
    },
    "bolt": {
      "enabled": true,
      "address": "localhost",
      "port": 7687
    },
    "csvDelimiter": ";"
  },
  "rtree": {
    "exportDir": "output/rtrees/",
    "nodeRef": 10
  },
  "batch": {
    "topLevel": 10,
    "feature": 100,
    "trans": 5000,
    "logTopLevel": 100
  },
  "multithreading": {
    "enabled": false,
    "producers": 1,
    "consumers": 7,
    "splitTopLevel": true,
    "timeout": 3600
  },
  "mapper": {
    "oldFile": "input/building_xs_0.xml",
    "newFile": "input/building_xs_1.xml",
    "fullName": true,
    "rules": {
      "citygml": "src/main/java/components/mapper/rules/citygml.json",
      "gml": "src/main/java/components/mapper/rules/gml.json",
      "xal": "src/main/java/components/mapper/rules/xal.json"
    }
  },
  "matcher": {
    "fuzzy": {
      "length": 1e-3,
      "angle": 1e-3,
      "shared": {
        "volume": true,
        "threshold": 0.9
      }
    }
  }
}